<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.calmlee.lesson04.dao.SellerDAO">

	<insert id="insertNewSeller" parameterType="map">
		INSERT INTO `new_user`
		(
			`nickname`
			,`profileImageUrl`
			,`temperature`
			,`createdAt`
			,`updatedAt`		
		)
		VALUES
		(
			#{nickname}
			,#{profileImageUrl}
			,#{temperature}
			,now()
			,now()
		)	
	</insert>
</mapper>